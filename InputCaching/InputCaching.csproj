<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Authors>NithinSuhas</Authors>
    <Description>solution to add input caching to your web api project</Description>
    <Copyright>open source</Copyright>
    <PackageProjectUrl>https://github.com/Nithinsuhas/Input-Caching</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Nithinsuhas/Input-Caching</RepositoryUrl>
    <PackageTags>input;caching;webapi;duplicatedPost;requests;webapicaching;.net;core;</PackageTags>
    <NeutralLanguage>en</NeutralLanguage>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Concretes\_MemoryCache.cs" />
    <Compile Remove="Concretes\_MemoryObject.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <None Include="docs\README.md" Pack="true" PackagePath="\" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\.NetFramework\InputCaching.net.framework\InputCaching.net.framework.csproj" />
  </ItemGroup>

</Project>
